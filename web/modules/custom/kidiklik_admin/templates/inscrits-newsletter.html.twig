Nombre d'inscrits : {{ count_list }}
<a href="/admin/newsletters/export_inscrit">Exporter</a>
<form method="post">
<div class="form-item form-type-textfield">
<div class="row">
<div class="col-xs-6">
<input type="text" class="form-control" name="email" id="email" placeholder="Recherche email"  value="{{ email }}" />
</div>
<div class="col-xs-6">
<input type="submit" value="Rechercher" />
</div>
</form>

<table class="views-table views-view-table cols-7 sticky-enabled table table-bordered table-striped responsive">
  <thead>
  <tr>
    <th class="select-all views-field views-field-node-bulk-form" scope="col"></th>
    <th id="view-title-table-column" class="views-field views-field-title" scope="col">Email</th>
    <th id="view-title-table-column" class="views-field views-field-title" scope="col">Nom</th>
    <th id="view-title-table-column" class="views-field views-field-title" scope="col">Prénom</th>
    <th id="view-field-date-de-fin-table-column" class="views-field views-field-field-date-de-fin" scope="col">Date inscription</th>

  </tr>
  </thead>
  <tbody id="dtable">

    {% for item in list %}
      <tr>
        <td></td>
        <td>{{ item.email }}</td>

        <td>{{ item.nom }}</td>
        <td>{{ item.prenom }}</td>
        <td>{{ item.inscription }}</td>
      </tr>

    {%  endfor %}

  </tbody>
  {% if count_list > 20 %}
  <tfoot>
    <tr>
      <td align="left" colspan="2">
        <a href="?start=0&step={{ step }}">Premier</a>

        <a href="?start={{ prec }}&step={{ step }}">Précédent</a>
      </td>

      <td></td>
      <td align="right" colspan="2">
        <a href="?start={{ suiv }}&step={{ step }}">Suivant</a>

        <a href="?start={{ count_list }}&step={{ step }}">Dernier</a>
      </td>
    </tr>
  </tfoot>
  {% endif %}
</table>